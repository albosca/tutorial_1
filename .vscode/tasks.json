{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake native",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"command": "cmake",
			"args": [
				"-Bbuild/native",
				"-DPLATFORM=native",
			]
		},
		{
			"label": "Build all native",
			"dependsOn": [
				"CMake native"
			],
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build"
			},
			"linux": {
				"command": "make"
			},
			"osx": {
				"command": "make"
			},
			"windows": {
				"command": "mingw32-make"
			},
			"args": [
				"-Cbuild/native",
			]
		},
		{
			"label": "CMake nucleo_stm32F446re",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"command": "cmake",
			"args": [
				"-Bbuild/nucleo_stm32F446re",
				"-DPLATFORM=nucleo_stm32F446re",
			]
		},
		{
			"label": "Build all nucleo_stm32F446re",
			"dependsOn": [
				"CMake nucleo_stm32F446re"
			],
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build"
			},
			"linux": {
				"command": "make"
			},
			"osx": {
				"command": "make"
			},
			"windows": {
				"command": "mingw32-make"
			},
			"args": [
				"-Cbuild/nucleo_stm32F446re",
			]
		},
		{
			"label": "Clean",
			"command": "make",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"linux": {
				"command": "make"
			},
			"osx": {
				"command": "make"
			},
			"windows": {
				"command": "mingw32-make"
			},
			"args": [
				"clean"
			]
		},
		{
			"label": "Clean & Re-Build NUCLEO_STM32F446RE",
			"dependsOn": [
				"Clean",
				"Build NUCLEO_STM32F446RE"
			],
			"dependsOrder": "sequence"
		},
		{
			"label": "Clean & Re-Build PC",
			"dependsOn": [
				"Clean",
				"Build PC"
			],
			"dependsOrder": "sequence"
		},
		{
			"label": "Flash NUCLEO_STM32F446RE",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"linux": {
				"command": "make"
			},
			"osx": {
				"command": "make"
			},
			"windows": {
				"command": "mingw32-make"
			},
			"args": [
				"flash",
				"PLATFORM=nucleo_stm32f446re"
			],
			"problemMatcher": []
		}
	]
}