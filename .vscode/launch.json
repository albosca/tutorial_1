{
  "version": "0.2.0",
  "env": {
    "SDGWorkspaceFolder": "${workspaceFolder}/../workspace"
  },
  "configurations": [
    {
      "name": "Debug device STM32F446RE",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/bin-debug/nucleo_stm32f446re/${input:targetID}.elf",
      "request": "launch",
      "runToEntryPoint": "main",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F446RE",
      "configFiles": [
        "${SDGWorkspaceFolder}/openocd/stm32f4x.cfg"
      ],
      "preLaunchTask": "Build (cortex-debug only)",
      "svdFile": "${SDGWorkspaceFolder}/svd/STM32F446.svd",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 16000000,
        "swoFrequency": 2000000,
        "source": "probe",
        "decoders": [
          {
            "type": "console",
            "label": "ITM",
            "port": 0
          }
        ]
      }
    }
  ],
  "inputs": [
		{
			"id": "targetID",
      "type": "command",
      "command": "extension.commandvariable.pickStringRemember",
      "args": {
        "key": "lasttarget",
        "description": "Enter the target:",
        "default": "project",
        "options": [
          "project",
          "hw_test_led"
        ]
      }
		},
	]
}
